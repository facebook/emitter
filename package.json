{
  "name": "fbemitter",
  "version": "2.0.0",
  "keywords": [
    "clientside"
  ],
  "bugs": "https://github.com/facebook/emitter/issues",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "files": [
    "README.md",
    "main.js",
    "bin/jsx",
    "build/React.js",
    "vendor/fbtransform/",
    "vendor/woodchipper.js"
  ],
  "main": "main.js",
  "bin": {
    "jsx": "./bin/jsx"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/facebook/emitter"
  },
  "scripts": {
    "test": "jest"
  },
  "dependencies": {
    "commoner": "^0.10.0",
    "jstransform": "^9.1.0"
  },
  "devDependencies": {
    "base62": "~0.1.1",
    "browserify": "^8.1.0",
    "esprima-fb": "^12001.1.0-dev-harmony-fb",
    "grunt": "~0.4.1",
    "grunt-compare-size": "~0.4.0",
    "grunt-contrib-clean": "~0.4.1",
    "grunt-contrib-compress": "~0.5.1",
    "grunt-contrib-copy": "~0.4.1",
    "grunt-contrib-jshint": "~0.5.4",
    "gzip-js": "~0.3.2",
    "jest-cli": "^0.3.0",
    "object-assign": "^2.0.0",
    "optimist": "~0.4.0",
    "phantomjs": ">= 1.9.0",
    "react-tools": "^0.12.2",
    "recast": "^0.9.11",
    "semver": ">= 1.1.4",
    "source-map": "~0.1.22",
    "uglify-js": "~2.4.16",
    "wrapup": "~0.12.0"
  },
  "preferGlobal": true,
  "commonerConfig": {
    "version": 1
  },
  "jest": {
    "rootDir": "",
    "scriptPreprocessor": "jest/preprocessor.js",
    "setupEnvScriptFile": "jest/environment.js",
    "persistModuleRegistryBetweenSpecs": true,
    "modulePathIgnorePatterns": [
      "/build/",
      "/node_modules/",
      "/.module-cache/"
    ],
    "testPathIgnorePatterns": [
      "/build/",
      "/node_modules/"
    ]
  },
  "eslintConfig": {
    "env": {
      "jasmine": true
    },
    "globals": {
      "__DEV__": true,
      "jest": true,
      "require": true,
      "module": true
    },
    "rules": {
      "quotes": 0,
      "no-reserved-keys": 0,
      "no-underscore-dangle": 0
    }
  }
}
